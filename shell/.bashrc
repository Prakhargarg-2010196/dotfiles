
# Aliases
alias vi="nvim" 
alias vim="nvim"
alias rm="rm -i" # to add interactive remove 
alias sysi="inxi -Fxz" # to check for system info
alias kern="uname -mrs" # to check current version of linux kernel 
#Export paths
export GIT_EDITOR=vim

#Custom Host paths
export PS1="\W \$ >"

# Env Paths
export PATH=$HOME/bin:/usr/local/bin:$PATH

# path to zsh 
export ZSH="$HOME/.oh-my-zsh"

#path to google-cloud
export GCLOUD_PROJECT=qa-kavachone
